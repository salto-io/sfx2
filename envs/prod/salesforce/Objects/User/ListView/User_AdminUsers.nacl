salesforce.ListView User_AdminUsers {
  fullName = "User.AdminUsers"
  columns = [
    "FULL_NAME",
    "CORE.USERS.ALIAS",
    "CORE.USERS.USERNAME",
    "CORE.USERS.LAST_LOGIN",
    "CORE.USER_ROLE.NAME",
    "CORE.USERS.ACTIVE",
  ]
  filterScope = "Everything"
  filters = [
    {
      field = "CORE.PROFILE.NAME"
      operation = "equals"
      value = "System Administrator"
    },
  ]
  label = "Admin Users"
  _parent = salesforce.User
}
